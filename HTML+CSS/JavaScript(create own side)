//створення перевіряючої функції на кілткість символів
function check_out(el){
    var name = el.name.value;
    var email = el.email.value;
    var password = el.password.value;
    var error = '';
    if(name.length < 4)
        error = 'name is small';
    if(email.length < 5)
        error = 'email is small';
    if(password.length < 8)
        error = 'password is small';
    console.log(error);
}
